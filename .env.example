# Empathy Engine Configuration

# TTS Engine Settings
# Set to "true" to use ElevenLabs API (for final testing only)
USE_ELEVENLABS=false

# ElevenLabs API Key (only needed if USE_ELEVENLABS=true)
ELEVENLABS_API_KEY=your_api_key_here

# ElevenLabs Voice ID (optional, uses default if not set)
ELEVENLABS_VOICE_ID=

# Server Settings
HOST=0.0.0.0
PORT=8000
DEBUG=true

# Audio Output Settings
OUTPUT_DIR=../output
AUDIO_FORMAT=wav
